    <!-- These are audio route (FE to BE) specific mixer settings -->
    
    <!-- HiFi -->
    <path name="deep-buffer-playback">
        <ctl name="SLIMBUS_0_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiSpeaker -->
    <path name="deep-buffer-playback speaker">
        <ctl name="MI2S_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiSpeaker -->
    <path name="deep-buffer-playback speaker">
        <path name="deep-buffer-playback" />
        <path name="deep-buffer-playback speaker" />
    </path>

    <!-- HiFiBT SCO Rx -->
    <path name="deep-buffer-playback bt-sco">
        <ctl name="INTERNAL_BT_SCO_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiBT SCO WB Rx -->
    <path name="deep-buffer-playback bt-sco-wb">
        <ctl name="INTERNAL_BT_SCO_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiPROXY Rx -->
    <path name="deep-buffer-playback afe-proxy">
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiUSB PROXY Rx -->
    <path name="deep-buffer-playback usb-headphones">
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiUSB PROXY Rx Speaker -->
    <path name="deep-buffer-playback usb-speaker">
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia1" value="1" />
        <ctl name="MI2S_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFi PROXY Rx Speaker -->
    <path name="deep-buffer-playback afe-proxy-speaker">
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia1" value="1" />
        <ctl name="MI2S_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFi Rec -->
    <path name="audio-record">
        <ctl name="MultiMedia1 Mixer SLIM_0_TX" value="1" />
    </path>

    <!-- HiFi RecBT SCO Tx -->
    <path name="audio-record bt-sco">
        <ctl name="MultiMedia1 Mixer INTERNAL_BT_SCO_TX" value="1" />
    </path>

    <!-- HiFi RecBT SCO WB Tx -->
    <path name="audio-record bt-sco-wb">
        <ctl name="MultiMedia1 Mixer INTERNAL_BT_SCO_TX" value="1" />
    </path>

    <!-- HiFi RecUSB PROXY Tx -->
    <path name="audio-record usb-headset-mic">
        <ctl name="MultiMedia1 Mixer AFE_PCM_TX" value="1" />
    </path>


